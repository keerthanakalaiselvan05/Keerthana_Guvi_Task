package com.Guvi;

public class ReverseNumber {

	public static void main(String[] args) {
		//Reverse the given number
		int num=876;
		System.out.println("Given Number is : "+num);
		int rem,rev=0;
		while(num!=0)
		{
		 rem=num%10;
		 rev=rev*10+rem;
		 num=num/10;
		}
       System.out.println("Reversed Number is : "+rev);
	}
}

